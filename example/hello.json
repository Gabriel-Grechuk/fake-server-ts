{
  "name": "Module name",
  "routes": [
    {
      "route": "/person",
      "filters": {}
    },
    {
      "route": "/person/:id",
      "filters": {
        "equal": {
          "personal_number": "id"
        }
      }
    },
    {
      "route": "/person-age",
      "filters": {
        "equal": {
          "age": "age"
        },
        "gte": {
          "age": "age-bigger-than"
        }
      }
    },
    {
      "route": "/person-age/:age",
      "filters": {
        "equal": {
          "age": "age"
        }
      }
    }
  ],

